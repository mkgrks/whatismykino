{
  "extends": [
    "phaistos-networks"
  ],
  "rules": {
    "func-names": 0,
    "one-var": 0,
    "quote-props": 0,
    "max-len": 0,
    "camelcase": 0,
    "one-var-declaration-per-line": 0,
    "no-unused-expressions": 0,
    "prefer-template": 0,
    "max-classes-per-file": true,
    "max-lines-per-function": true,
    "no-debugger": 1
  },
  "settings": {},
  "globals": {},
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "env": {
    "jest": true
  }
}